{
    "name": "clearcode/eh-library",
    "description": "Model and use cases for rest api course",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Clearcode",
            "homepage": "http://clearcode.cc/"
        }
    ],
    "autoload": {
        "psr-4" : { "Clearcode\\EHLibrary\\": "src" }
    },
    "autoload-dev": {
        "psr-4" : {
            "tests\\Clearcode\\EHLibrary\\" : "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.5",
        "ramsey/uuid": "~3.0",
        "everzet/persisted-objects": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "behat/behat": "~2.5",
        "satooshi/php-coveralls": "^0.6.1"
    }
}
